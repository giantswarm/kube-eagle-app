{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "containerSecurityContext": {
            "type": "object"
        },
        "env": {
            "type": "object"
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "registry": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "ingress": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "enabled": {
                    "type": "boolean"
                },
                "hosts": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "path": {
                    "type": "string"
                },
                "tls": {
                    "type": "array"
                }
            }
        },
        "livenessProbe": {
            "type": "object",
            "properties": {
                "timeoutSeconds": {
                    "type": "integer"
                }
            }
        },
        "logLevel": {
            "type": "string"
        },
        "metricsNamespace": {
            "type": "string"
        },
        "nodeSelector": {
            "type": "object"
        },
        "podAnnotations": {
            "type": "object"
        },
        "podSecurityContext": {
            "type": "object",
            "properties": {
                "fsGroup": {
                    "type": "integer"
                },
                "runAsUser": {
                    "type": "integer"
                }
            }
        },
        "priorityClassName": {
            "type": "string"
        },
        "project": {
            "type": "object",
            "properties": {
                "branch": {
                    "type": "string"
                },
                "commit": {
                    "type": "string"
                }
            }
        },
        "rbac": {
            "type": "object",
            "properties": {
                "create": {
                    "type": "boolean"
                }
            }
        },
        "readinessProbe": {
            "type": "object",
            "properties": {
                "timeoutSeconds": {
                    "type": "integer"
                }
            }
        },
        "replicaCount": {
            "type": "integer"
        },
        "resources": {
            "type": "object"
        },
        "service": {
            "type": "object",
            "properties": {
                "port": {
                    "type": "integer"
                },
                "type": {
                    "type": "string"
                }
            }
        },
        "serviceAccountName": {
            "type": "string"
        },
        "serviceMonitor": {
            "type": "object",
            "properties": {
                "additionalLabels": {
                    "type": "null"
                },
                "create": {
                    "type": "boolean"
                },
                "interval": {
                    "type": "string"
                },
                "releaseLabel": {
                    "type": "string"
                },
                "scrapeTimeout": {
                    "type": "string"
                }
            }
        },
        "serviceType": {
            "type": "string"
        },
        "telemetry": {
            "type": "object",
            "properties": {
                "host": {
                    "type": "string"
                },
                "port": {
                    "type": "integer"
                }
            }
        },
        "tolerations": {
            "type": "array"
        }
    }
}
